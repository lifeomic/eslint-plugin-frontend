{
  "name": "@lifeomic/eslint-plugin-frontend",
  "version": "0.0.0",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "repository": "git@github.com:lifeomic/eslint-plugin-frontend.git",
  "author": "Tony Ward <tony.ward@lifeomic.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16 || ^14.17"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "eslint",
    "rules",
    "frontend"
  ],
  "homepage": "https://github.com/lifeomic/eslint-plugin-frontend#readme",
  "scripts": {
    "changelog": "lerna-changelog",
    "test": "jest"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "eslint": "^7.7.0",
    "jest": "^26.4.0",
    "lerna-changelog": "^1.0.1",
    "semantic-release": "^19.0.2"
  },
  "jest": {
    "testMatch": [
      "**/tests/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 96,
        "functions": 99,
        "lines": 98,
        "statements": 98
      }
    }
  }
}
